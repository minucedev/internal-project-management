# Copy file này thành .env và điền thông tin thực

# API Configuration
VITE_API_BASE_URL=http://localhost:8080/api
VITE_API_TIMEOUT=10000

# Authentication
VITE_TOKEN_STORAGE_KEY=pms_auth_token
VITE_USER_STORAGE_KEY=pms_auth_user
VITE_TOKEN_EXPIRY_MINUTES=15

# Firebase Configuration (for real-time chat - Phase 2)
VITE_FIREBASE_API_KEY=
VITE_FIREBASE_AUTH_DOMAIN=
VITE_FIREBASE_PROJECT_ID=
VITE_FIREBASE_STORAGE_BUCKET=
VITE_FIREBASE_MESSAGING_SENDER_ID=
VITE_FIREBASE_APP_ID=
VITE_FIREBASE_DATABASE_URL=

# App Configuration
VITE_APP_NAME=Project Management System
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_CHAT=false
VITE_ENABLE_PRESENCE=false
VITE_ENABLE_NOTIFICATIONS=true

# Development
VITE_ENABLE_REDUX_DEVTOOLS=true
VITE_LOG_LEVEL=debug